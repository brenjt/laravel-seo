{
    "name": "zfort/:package_name",
    "description": ":package_description",
    "type": "library",
    "keywords": [
        "zfort",
        ":package_name"
    ],
    "homepage": "https://github.com/zfort/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "developer@zfort.com",
            "homepage": "https://zfort.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ZFort\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFort\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZFort\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "ZFort\\Skeleton\\SkeletonFacade"
            }
        }
    }
}
